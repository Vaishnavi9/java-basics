package SeparateFunction;

import java.awt.List;
import java.util.*;

public class Test {
	
	
	
	public static void main(String[] args) {
		
		java.util.List<Integer> l=new ArrayList<Integer>();
		
		l.add(2);
		l.add(3);
		l.add(4);
		
		
		
		if(l.contains(2)) {
			System.out.println("YES");
		}
	}

}
